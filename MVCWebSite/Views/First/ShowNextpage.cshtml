
@{
    ViewBag.Title = "ShowNextpage";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Operation</title>
</head>
<body>
    <div>
        @(ViewBag.Id)<br/>
        
        @for (int i = 1; i <= 10; i++)
        {
            int result = i * ViewBag.Id;
            <font color="red" size="4">@ViewBag.id*@i=@result</font>
            <br />
        }
    </div>
</body>
</html>
